{
  "name": "islamgaliyev/github-kanban-board",
  "description": "Kanban board for Github issues",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "require": {
    "php": "^8.1",
    "mustache/mustache": "^2.9",
    "guzzlehttp/guzzle": "7.4",
    "knplabs/github-api": "v3.5.1",
    "michelf/php-markdown": "^1.6",
    "vlucas/phpdotenv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true
}
