{
  "name": "islamgaliyev/github-kanban-board",
  "description": "Kanban board for Github issues",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "require": {
    "php": "^8.1",
    "mustache/mustache": "^2.9",
    "guzzlehttp/guzzle": "^7.0.1",
    "knplabs/github-api": "^3.5.0",
    "michelf/php-markdown": "^1.6",
    "vlucas/phpdotenv": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true
}
