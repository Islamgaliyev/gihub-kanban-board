{
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"require": {
		"mustache/mustache": "^2.9",
		"php-http/guzzle6-adapter": "v2.0.2",
		"knplabs/github-api": "*",
		"michelf/php-markdown": "^1.6",
		"vlucas/phpdotenv": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "5.0.*"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"prefer-stable": true
}
